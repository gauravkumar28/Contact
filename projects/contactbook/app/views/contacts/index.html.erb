<div class="row-fluid">
  <div class="col-mx-3">
  </div>
  <div class="col-mx-6">
    <div class="table-responsive">
      <table class="table table-hover">
	      <tr>
			    <th> Name </th>
			    <th> Phone </th>
			    <th> Email </th>
			  </tr>
		    <% @contacts.each do |contact| %>
		      <tr>
				    <td><%= link_to contact.name, contact_path(contact) %></td>
				    <td><%= contact.phone %></td>
				    <td><%= contact.emails.pluck(:email_url).join(",") %></td>
				  </tr>
		    <% end %>
      </table>
    </div>
  </div>
</div>

